                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	a.CMPNBR as Compania,
	a.SPID as Cod_Suplidor, 
	a.INVID as Documento,
	b.ATID as Term_Pago, 
	a.AYID Tipo_Documento, 
	b.CURCDE as Moneda, 
	a.BKCO as Cod_Banco, 
	b.poid, --a.BKCPC as CashProfitCenter, 
	a.BANKID Cuenta_Banco,
	b.IHSTAT as Estatus_Factura, 
	--a.M$sts as EstatusDetalle,
	b.IHDESC as Detalle, 
	b.IHINVD as Fecha_DocumentoFactura, 
	b.IHESTD as Fecha_IngresoFactura,
	a.DDDUED as Fecha_Vencimiento,
	--a.IHEXRT as Rata,
	sum(a.DDDAMT) as Monto_Original,--CREDITO
	sum(a.DDPAMT) as Monto_Pagado,-- DEBITO
	sum(a.DDDAMC) as PendienteXPagar,
	--(sum(a.DDDAMT)-sum(a.DDPAMT)) as calculoPendiente
	((sum(a.DDDAMT)-sum(a.DDPAMT )) * max(b.IHEXRT)) as calculoPendiente,
	max(b.IHEXRT) as Tasa_Cambio_Moneda
FROM APMDDP a JOIN APMIHP b ON a.CMPNBR= b.CMPNBR AND a.SPID =b.SPID AND a.INVID=b.INVID
WHERE  b.IHINVD &gt;=20180101
GROUP BY 
a.CMPNBR,a.SPID, a.INVID, b.ATID,a.AYID, b.CURCDE,a.BKCO,b.poid, a.BANKID,b.IHSTAT,b.IHINVD,a.DDDUED,b.IHESTD,b.IHDESC</property>
