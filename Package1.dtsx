            <component
              refId="Package\Data Flow Task\PARAMETRO 6 IDXSETTING"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="PARAMETRO 6 IDXSETTING"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 'D',       a.oiline,       CASE WHEN PWP.P$PWWIM IS NULL THEN a.ITEM              ELSE PWP.P$PWWIM       END AS ITEM,       a.replt,       (AA.IAAQTY - ifnull(cred.M$QRVC, 0)) rersqy,         a.coid,       Substring(bb.rhrsdt, 1, 4) || '-' || Substring(bb.rhrsdt, 5, 2) || '-' || Substring(bb.rhrsdt, 7, 2) fecha,         bb.cusid,       bb.trnsmd,       bb.relsts,       CASE WHEN ohref LIKE 'DEL PO:%' THEN O.ohref              ELSE bb.rhcpo       END rhcpo,       bb.plant,       bb.cusid AS Cliente,       bb.RHCPO COMENTARIOS,       x.CMNAME,       s.OESTNM AS Consigned,       (SELECT LISTAGG(TRIM(UCASE(onnote)), ', ') WITHIN GROUP(ORDER BY onseq asc)            FROM OETOPL97  H         INNER          JOIN OETONP D            ON D.coid = H.coid             AND H.LANG = D.LANG           AND H.ONNID = D.ONNID         WHERE h.coid = bb.coid             and h.pacode = 'PKSL'           AND ifnull(onnote, '') &lt;&gt; ''             AND onseq &lt;= 65) t019,       (Select Substring(hp.ohrfdt, 1, 4) || '-' || Substring(hp.ohrfdt, 7, 2) || '-' || Substring(hp.ohrfdt, 5, 2) FechaDesp            FROM oetohp hp         WHERE hp.coid = Bb.coid),       bp.cbnam As ConsigNombre,         (LTrim(RTrim(bp.cbadr1))) || ' ' || (LTrim(RTrim(bp.cbadr2))) || ' ' || (LTrim(RTrim(bp.cbadr3))) As ConsigDirec,         (LTrim(RTrim(bp.cbcity))) As ConsigCiudad,       (LTrim(RTrim(bp.cntry))) As ConsigCountry,         (LTrim(RTrim(aa.whse))) As Whse,       LTrim(RTrim(bb.trncar)) As TrnCar,       (LTrim(RTrim(cred.iyid))) As GrupCom,         a.ITEM AS ItemOrig,       s.OESTA1,       s.OESTA2  FROM p$wrep a  JOIN P$WRAP aa    on a.coid = aa.coid   AND a.OILINE = aa.oiline     and a.item = aa.item   and a.RLSNBR = aa.RLSNBR  JOIN p$wrhp  bb    on a.coid = bb.coid INNEr  JOIN OETSAP s      ON bb.cusid = s.cusid   AND bb.coid = s.coid INNER  JOIN OETOIP cred    ON cred.coid = a.coid   AND cred.OILINE = a.oiline    JOIN XAMCMP x    ON x.cusid = Bb.cusid  JOIN XAMCBP BP    ON bb.cusid = bp.cusid   and bb.rhblid = bp.billid      LEFT outer  JOIN P$MPWP PWP    ON PWP.P$PWITM = A.ITEM   AND PWP.P$PWPLT = bb.PLANT  LEFT  JOIN OETOHP O    ON O.COID = BB.COID   AND O.PLANT = A.replt Where 1 = 1   And Bb.plant In ('FF', 'ME', 'EL', 'CO')   And (AA.IAAQTY - ifnull(cred.M$QRVC, 0)) &gt; 0     AND bb.trncar NOT IN ('ETL')</property>
