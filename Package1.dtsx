        <pipeline
          version="1">
          <components>
            <component
              refId="Package\Data Flow Task\PARAMETRO 6"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="PARAMETRO 6"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	'D',
	a.oiline,
	CASE
		WHEN PWP.P$PWWIM IS NULL THEN a.ITEM
		ELSE PWP.P$PWWIM
	END AS ITEM,
	a.replt,
	(AA.IAAQTY - ifnull(cred.M$QRVC,
	0)) rersqy,
	a.coid,
	SUBSTRING(bb.rhrsdt, 1, 4) || '-' || SUBSTRING(bb.rhrsdt, 5, 2) || '-' || SUBSTRING(bb.rhrsdt, 7, 2) fecha,
	bb.cusid,
	bb.trnsmd,
	bb.relsts,
	CASE
		WHEN ohref LIKE 'DEL PO:%' THEN O.ohref
		ELSE bb.rhcpo
	END rhcpo,
	bb.plant,
	bb.cusid AS Cliente,
	bb.RHCPO COMENTARIOS,
	x.CMNAME,
	s.OESTNM AS Consigned,
	(
	SELECT
		LISTAGG(TRIM(UCASE(onnote)),
		', ') WITHIN GROUP(
		ORDER BY onseq ASC)
	FROM
		MOPDTA.OETOPL97  H
	INNER JOIN MOPDTA.OETONP D ON
		D.coid = H.coid
		AND H.LANG = D.LANG
		AND H.ONNID = D.ONNID
	WHERE
		h.coid = bb.coid
		AND h.pacode = 'PKSL'
		AND ifnull(onnote,
		'') &lt;&gt; ''
		AND onseq &lt;= 65) t019,
	(
	SELECT
		SUBSTRING(hp.ohrfdt, 1, 4) || '-' || SUBSTRING(hp.ohrfdt, 7, 2) || '-' || SUBSTRING(hp.ohrfdt, 5, 2) FechaDesp
	FROM
		MOPDTA.oetohp hp
	WHERE
		hp.coid = Bb.coid),
	bp.cbnam AS ConsigNombre,
	(LTrim(RTrim(bp.cbadr1))) || ' ' || (LTrim(RTrim(bp.cbadr2))) || ' ' || (LTrim(RTrim(bp.cbadr3))) AS ConsigDirec,
	(LTrim(RTrim(bp.cbcity))) AS ConsigCiudad,
	(LTrim(RTrim(bp.cntry))) AS ConsigCountry,
	(LTrim(RTrim(aa.whse))) AS Whse,
	LTrim(RTrim(bb.trncar)) AS TrnCar,
	(LTrim(RTrim(cred.iyid))) AS GrupCom,
	a.ITEM AS ItemOrig,
	s.OESTA1,
	s.OESTA2
FROM
	MOPDTA.p$wrep a
JOIN MOPDTA.P$WRAP aa ON
	a.coid = aa.coid
	AND a.OILINE = aa.oiline
	AND a.item = aa.item
	AND a.RLSNBR = aa.RLSNBR
JOIN MOPDTA.p$wrhp  bb ON
	a.coid = bb.coid
INNER JOIN MOPDTA.OETSAP s ON
	bb.cusid = s.cusid
	AND bb.coid = s.coid
INNER JOIN MOPDTA.OETOIP cred ON
	cred.coid = a.coid
	AND cred.OILINE = a.oiline
JOIN MOPDTA.XAMCMP x ON
	x.cusid = Bb.cusid
JOIN MOPDTA.XAMCBP BP ON
	bb.cusid = bp.cusid
	AND bb.rhblid = bp.billid
LEFT OUTER JOIN MOPDTA.P$MPWP PWP ON
	PWP.P$PWITM = A.ITEM
	AND PWP.P$PWPLT = bb.PLANT
LEFT JOIN MOPDTA.OETOHP O ON
	O.COID = BB.COID
	AND O.PLANT = A.replt
WHERE
	1 = 1
	AND Bb.plant IN ('FF', 'ME', 'EL', 'CO')
	AND (AA.IAAQTY - ifnull(cred.M$QRVC,
	0)) &gt; 0
	AND bb.trncar NOT IN ('ETL')
	AND O.COID = 1414262</property>
